<%- include("partials/header"); -%>
    <h1>Contact</h1>
    <p> <%= contactContent %> </p>
    <!-- <a class="contactme" href="mailto:contacthemrajjat@gmail.com" target="_blank">Mail me</a> -->
    <form action="/contactmessage" method="POST">
        <input class="form-control" type="text" name="name" id="" placeholder="Your Name" required autocomplete="off">
        <br>
        <input class="form-control" type="email"name="mailid" id="" placeholder="Your email" required autocomplete="off">
        <br>
        <textarea class="form-control" name="message" id="" cols="80" rows="10" placeholder="Write you message ..." required autocomplete="off"></textarea>
        <br>
        <button class="btn btn-primary" type="submit" >Send</button>
    </form>
<%- include("partials/footer"); -%>
